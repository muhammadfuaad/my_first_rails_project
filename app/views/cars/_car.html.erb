<div id="<%= dom_id car %>">
  <p>
    <strong>Name:</strong>
    <%= car.name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= car.price %>
  </p>

  <p>
    <strong>Model:</strong>
    <%= car.model %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= car.location %>
  </p>

  <p>
    <strong>Usage:</strong>
    <%= car.usage %>
  </p>

  <p>
    <strong>Owner's email:</strong>
    <%= car.email %>
  </p>

</div>


